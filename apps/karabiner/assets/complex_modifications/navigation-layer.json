            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "y",
            "modifiers": {
              "mandatory": [
                "left_shift",
                "left_command",
                "left_control",
                "left_option"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_arrow",
              "modifiers": ["left_control"]
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "g",
            "modifiers": {
              "mandatory": [
                "left_shift",
                "left_command",
                "left_control",
                "left_option"
              ]
            }
          },
          "to": [
            {
              "key_code": "open_bracket",
              "modifiers": ["left_command", "left_shift"]
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "h",
